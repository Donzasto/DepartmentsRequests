<Application x:Class="DepartmentsRequests.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:userControlsDataGrids="clr-namespace:DepartmentsRequests.Views.UserControls.DataGrids"
			 xmlns:viewmodels="clr-namespace:DepartmentsRequests.ViewModels"
             StartupUri="MainWindow.xaml">
	<Application.Resources>
		<DataTemplate DataType="{x:Type viewmodels:OGMehViewModel}">
			<userControlsDataGrids:OGMehDataGridUserControl/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewmodels:BPUViewModel}">
			<userControlsDataGrids:BPUDataGridUserControl/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewmodels:ETsViewModel}">
			<userControlsDataGrids:ETsDataGridUserControl/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewmodels:KBSTViewModel}">
			<userControlsDataGrids:KBSTDataGridUserControl/>
		</DataTemplate>
	</Application.Resources>
</Application>
